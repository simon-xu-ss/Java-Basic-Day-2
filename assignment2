public class day2Assignment2 {
	public static void main(String[] args) {
		int[][] numbers = { {1, 2, 3, 4}, {5, 6, 7} };
		
		int max = Integer.MIN_VALUE;
		
		for (int i = 0; i < numbers.length; i++) {
			for (int j = 0; j < numbers[i].length; j++) {
				if (numbers[i][j] > max) {
					max = numbers[i][j];
				}
			}
		}
		
		System.out.println(max);
	}
}
